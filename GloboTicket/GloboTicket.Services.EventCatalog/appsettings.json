{
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\MSSQLLocalDB;Database=GloboTicketEventCatalog;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "d:\\GloboTicket.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
          "shared": true,
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
